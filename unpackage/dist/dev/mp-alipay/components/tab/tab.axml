<view class="wrapper"><view class="box"><scroll-view class="tab-box" scroll-x="true"><view class="tab-box__list"><view class="tab-box__list__item" a:for="{{list}}" a:for-item="item" a:for-index="index" a:key="index"><text data-event-opts="{{[['tap',[['e0',['$event']]]]]}}" data-event-params="{{({item,index})}}" class="{{((index===activeIndex)?'active':'')}}" onTap="__e">{{''+item.name+''}}</text></view></view></scroll-view><view data-event-opts="{{[['tap',[['handleLaberSet',['$event']]]]]}}" class="tab-set" onTap="__e"><text class="iconfont tab-set__icon">î ƒ</text></view></view></view>