<view data-event-opts="{{[['tap',[['handleList',['$event']]]]]}}" class="list-cart" onTap="__e"><block a:if="{{item.mode==='base'}}"><view class="base"><image class="base__img" src="{{item.cover[0]}}" mode="aspectFill"></image><view class="base__content"><view class="base__content__title">{{item.title+''}}<list-like vue-id="3a4ea810-1" item="{{item}}" type="{{type}}" onVueInit="__l"></list-like></view><view class="base__content__des"><view class="base__content__des__label">{{item.classify}}</view><view class="base__content__des__scan">{{item.browse_count+"浏览"}}</view></view></view></view></block><block a:if="{{item.mode==='many'}}"><view class="many"><view class="many__content"><view class="many__content__title">{{item.title+''}}<list-like vue-id="3a4ea810-2" item="{{item}}" type="{{type}}" onVueInit="__l"></list-like></view><view class="many__content__imgs"><image class="many__content__img" src="{{item.cover[i]}}" mode="aspectFill" a:for="{{3}}" a:for-item="i" a:for-index="__i0__" a:key="*this"></image></view><view class="many__content__des"><view class="many__content__des__label">{{item.classify}}</view><view class="many__content__des__scan">{{item.browse_count+"浏览"}}</view></view></view></view></block><block a:if="{{item.mode==='big'}}"><view class="big"><view class="big__content"><image class="big__content__big-img" src="{{item.cover[0]}}" mode="aspectFill"></image><view class="big__content__title big-margin">{{item.title+''}}<list-like vue-id="3a4ea810-3" item="{{item}}" type="{{type}}" onVueInit="__l"></list-like></view><view class="big__content__des"><view class="big__content__des__label">{{item.classify}}</view><view class="big__content__des__scan">{{item.browse_count+"浏览"}}</view></view></view></view></block></view>